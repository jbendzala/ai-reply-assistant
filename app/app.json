{
  "expo": {
    "name": "AI Reply Assistant",
    "slug": "ai-reply-assistant",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0A0F"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aireplyassistant.app"
    },
    "android": {
      "package": "com.aireplyassistant.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0A0A0F"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "scheme": "ai-reply-assistant",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "967015e8-f985-447b-a3a4-c9b28f8a0146"
      }
    },
    "owner": "bendzalaj"
  }
}
